{
  "name": "antd4-pro-framework",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "start": "umi dev",
    "build": "umi build",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "commit": "git-cz"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write",
      "git add"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "git add"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "dependencies": {
    "@umijs/preset-react": "^1.5.17",
    "@umijs/test": "^3.2.9",
    "antd": "^4.4.2",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "lodash-es": "^4.17.15",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-responsive": "^8.1.0",
    "umi": "^3.2.9",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "@types/jest": "^26.0.4",
    "@types/lodash-es": "^4.17.3",
    "@types/qs": "^6.9.3",
    "@types/react-helmet": "^6.0.0",
    "@types/react-responsive": "^8.0.2",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "3.2.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "roadhog-api-doc": "^1.1.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
